<!-- global wait indicator -->
<style>
  .loader-div {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1100;
    background-color: white;
    opacity: .6;
  }
  .ajax-loader {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -32px; /* -1 * image width / 2 */
    margin-top: -32px; /* -1 * image height / 2 */
    display: block;
  }
</style>
<div class="loader-div" loader style="display:none;">
  <img src="/assets/images/loader-indicator.gif" class="ajax-loader" />
</div>

<!-- global growl placehoder -->
